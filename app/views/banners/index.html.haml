%h1 Listing banners

%table
  %tr
    %th Archivo
    %th Url
    %th Cliente
    %th
    %th
    %th

  - @banners.each do |banner|
    %tr
      %td= banner.archivo
      %td= banner.url
      %td= banner.cliente
      %td= link_to 'Show', banner
      %td= link_to 'Edit', edit_banner_path(banner)
      %td= link_to 'Destroy', banner, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

